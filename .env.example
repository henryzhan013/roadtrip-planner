# Copy this file to .env and fill in your values

# OpenAI API key for ChatGPT
# Get from: https://platform.openai.com/api-keys
OPENAI_API_KEY=your-openai-key-here

# Google Places API key
# Get from: https://console.cloud.google.com/apis/credentials
GOOGLE_PLACES_API_KEY=your-google-key-here

# Rate limiting (optional, these are defaults)
OPENAI_RATE_LIMIT_PER_MINUTE=60
OPENAI_RATE_LIMIT_PER_DAY=1000
GOOGLE_RATE_LIMIT_PER_MINUTE=60
GOOGLE_RATE_LIMIT_PER_DAY=1000
